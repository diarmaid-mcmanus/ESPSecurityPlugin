<!-- Java rules for Sink methods -->
<rules>
	<rule>Ljava/util/logging/Logger;.log(Ljava/util/logging/Level;Ljava/lang/String;)V</rule>
	<rule>Ljavax/servlet/ServletOutputStream;.print(Ljava/lang/String;)V|Ljava/io/IOException;</rule>
	<rule argument="false">Ljava/lang/Throwable;.printStackTrace()V</rule>
</rules>